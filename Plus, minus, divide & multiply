
#include<iostream>
using namespace std;
int main()
{
	int a,b;
  cout<<"Enter the first number : ";
	cin>>a;
 cout<<"Enter the second number : ";
	cin>>b;
  
  cout<<"Adddition : "<< a+b<< endl;
  cout<<"Substraction : "<< a-b<< endl;
  cout<<"Multiplication : "<< a*b<< endl;
 
  if(b!=0)
  cout<<"Division : "<< a/b;
  else
  cout<<"Division : invalid";
  
	
	return 0;
}
